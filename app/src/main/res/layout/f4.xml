<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">


  <EditText
      android:id="@+id/e1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_marginStart="119dp"
      android:layout_marginTop="50dp"
      android:ems="10"
      android:inputType="textPersonName" />

  <EditText
      android:id="@+id/e2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_marginStart="119dp"
      android:layout_marginTop="120dp"
      android:ems="10"
      android:inputType="number" />

  <Spinner
      android:id="@+id/spinner3"
      android:layout_width="211dp"
      android:layout_height="42dp"

      android:layout_alignParentStart="true"
      android:layout_alignParentBottom="true"
      android:layout_marginStart="121dp"
      android:layout_marginBottom="380dp" />

  <EditText
      android:id="@+id/e5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentBottom="true"
      android:layout_marginStart="121dp"
      android:layout_marginBottom="250dp"
      android:ems="10"
      android:inputType="textPersonName" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="199dp"
      android:layout_height="32dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_marginStart="8dp"
      android:layout_marginTop="6dp"
      android:text="Enter Student Details.."
      android:textColor="@color/orange"
      android:textSize="20sp"
      android:textStyle="bold|italic" />

  <EditText
      android:id="@+id/editText10"
      android:layout_width="wrap_content"
      android:layout_height="35dp"
      android:layout_below="@+id/e5"
      android:layout_alignParentStart="true"
      android:layout_marginStart="119dp"
      android:layout_marginTop="20dp"
      android:ems="10"
      android:inputType="textPassword" />

  <EditText
      android:id="@+id/editText15"
      android:layout_width="wrap_content"
      android:layout_height="0dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentBottom="true"
      android:layout_marginBottom="193dp"
      android:ems="10"
      android:layout_marginStart="119dp"
      android:inputType="textPassword" />

  <EditText
      android:id="@+id/e4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_above="@+id/e5"
      android:layout_alignParentStart="true"
      android:layout_marginStart="121dp"
      android:layout_marginBottom="25dp"
      android:ems="10"
      android:inputType="phone" />

  <EditText
      android:id="@+id/e6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignTop="@+id/editText15"
      android:layout_alignParentStart="true"
      android:layout_marginStart="121dp"
      android:layout_marginTop="-33dp"
      android:ems="10"
      android:inputType="textPassword" />

  <EditText
      android:id="@+id/e7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentBottom="true"
      android:layout_marginStart="124dp"
      android:layout_marginBottom="114dp"
      android:ems="10"
      android:inputType="textPassword" />

  <Button
      android:id="@+id/b1"
      android:layout_width="160dp"
      android:layout_height="wrap_content"
      android:layout_alignParentBottom="true"
      android:layout_marginStart="-75dp"
      android:layout_marginBottom="31dp"
      android:layout_toEndOf="@+id/textView8"
      android:text="Proceed"
      android:textSize="18sp"
      android:background="@drawable/buttond"
      android:textColor="#ffffff"/>

  <TextView
      android:id="@+id/textView8"
      android:layout_width="67dp"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_marginStart="28dp"
      android:layout_marginTop="65dp"
      android:text="Name :"
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView10"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignStart="@+id/textView7"
      android:layout_alignParentTop="true"
      android:layout_marginStart="22dp"
      android:layout_marginTop="135dp"
      android:text="Roll No:"
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignBottom="@+id/spinner3"
      android:layout_alignParentStart="true"
      android:layout_marginStart="28dp"
      android:layout_marginBottom="13dp"
      android:text="Class :"
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView12"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignBottom="@+id/e4"
      android:layout_alignParentStart="true"
      android:layout_marginStart="28dp"
      android:layout_marginBottom="9dp"
      android:text="Phone no."
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView13"
      android:layout_width="82dp"
      android:layout_height="wrap_content"
      android:layout_alignTop="@+id/e5"
      android:layout_alignEnd="@+id/textView12"
      android:layout_marginStart="28dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="-3dp"
      android:text="Email :"
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView14"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignStart="@+id/textView7"
      android:layout_alignBottom="@+id/e6"
      android:layout_marginStart="22dp"
      android:layout_marginBottom="11dp"
      android:text="Password :"
      android:textSize="18sp" />

  <TextView
      android:id="@+id/textView15"
      android:layout_width="99dp"
      android:layout_height="wrap_content"
      android:layout_alignStart="@+id/textView8"
      android:layout_alignParentBottom="true"
      android:layout_marginStart="2dp"
      android:layout_marginBottom="113dp"
      android:text="Confirm Password"
      android:textSize="18sp" />

  <Button
      android:id="@+id/b2"
      android:layout_width="160dp"
      android:layout_height="wrap_content"
      android:layout_alignTop="@+id/b1"
      android:layout_marginStart="10dp"
      android:layout_marginTop="0dp"
      android:layout_toEndOf="@+id/b1"
      android:text="Cancel"
      android:textSize="18sp"
      android:background="@drawable/buttond"
      android:textColor="#ffffff"/>

</RelativeLayout>
